package jdbc;

public class DAOTeste {

	public static void main(String[] args) {
		
		DAO dao = new DAO();
		
	String sql = "INSERT INTO pessoas (nome) VALUES (?)";
	System.out.println(dao.incluir(sql, "luciana da Silva"));
	dao.incluir(sql, "igor da Silva");
	dao.incluir(sql, "iago da Silva");
	
	
	}
}
